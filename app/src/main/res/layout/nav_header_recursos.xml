<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:n1="http://schemas.android.com/apk/res/android"
    n1:layout_width="fill_parent"
    n1:layout_height="@dimen/nav_header_height"
    n1:background="@drawable/side_nav_bar"
    n1:gravity="bottom"
    n1:orientation="vertical"
    n1:paddingBottom="@dimen/activity_vertical_margin"
    n1:paddingLeft="@dimen/activity_horizontal_margin"
    n1:paddingRight="@dimen/activity_horizontal_margin"
    n1:paddingTop="@dimen/activity_vertical_margin"
    n1:theme="@style/ThemeOverlay.AppCompat.Dark">

    <ImageView
        xmlns:n2="http://schemas.android.com/apk/res-auto"
        n1:id="@+id/imageView"
        n1:layout_width="81.0dip"
        n1:layout_height="76.0dip"
        n1:paddingTop="@dimen/nav_header_vertical_spacing"
        n2:srcCompat="@drawable/click_logo" />

    <TextView
        n1:layout_width="fill_parent"
        n1:layout_height="wrap_content"
        n1:paddingTop="@dimen/nav_header_vertical_spacing"
        n1:text=""
        n1:textAppearance="@style/TextAppearance.AppCompat.Body1" />

    <TextView
        n1:id="@+id/textView"
        n1:layout_width="wrap_content"
        n1:layout_height="wrap_content"
        n1:text="" />
</LinearLayout>