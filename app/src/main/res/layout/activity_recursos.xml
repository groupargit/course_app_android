<?xml version="1.0" encoding="utf-8"?>
<androidx.support.v4.widget.DrawerLayout xmlns:n1="http://schemas.android.com/apk/res/android"
    n1:id="@+id/drawer_layout"
    n1:layout_width="fill_parent"
    n1:layout_height="fill_parent"
    n1:fitsSystemWindows="true">

    <include
        layout="@layout/app_bar_recursos"
        n1:layout_width="fill_parent"
        n1:layout_height="fill_parent" />

    <androidx.support.design.widget.NavigationView xmlns:n2="http://schemas.android.com/apk/res-auto"
        n1:id="@+id/nav_view"
        n1:layout_width="wrap_content"
        n1:layout_height="fill_parent"
        n1:layout_gravity="start"
        n1:fitsSystemWindows="true"
        n2:headerLayout="@layout/nav_header_recursos"
        n2:menu="@menu/activity_recursos_drawer" />
</androidx.support.v4.widget.DrawerLayout>